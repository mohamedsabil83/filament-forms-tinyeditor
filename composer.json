{
    "name": "ap1969/filament-forms-tinyeditor",
    "description": "A tinymce editor field for Filament forms",
    "keywords": [
        "laravel",
        "filament",
        "forms",
        "tinyeditor",
        "tinymce"
    ],
    "homepage": "https://github.com/mohamedsabil83/filament-forms-tinyeditor",
    "license": "MIT",
    "authors": [
        {
            "name": "MohamedSabil83",
            "email": "me@mohamedsabil83.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "filament/forms": "^2.0",
        "illuminate/contracts": "^8.73|^9.0",
        "spatie/laravel-package-tools": "^1.9.2"
    },
    "require-dev": {
        "nunomaduro/collision": "^5.10|^6.1",
        "nunomaduro/larastan": "^1.0|^2.0",
        "orchestra/testbench": "^6.23|^7.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Mohamedsabil83\\FilamentFormsTinyeditor\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mohamedsabil83\\FilamentFormsTinyeditor\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true,
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mohamedsabil83\\FilamentFormsTinyeditor\\FilamentFormsTinyeditorServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
